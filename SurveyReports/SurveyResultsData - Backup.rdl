      <Query>
        <DataSourceName>TeaParty</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
   q.question
   ,sr.result
FROM Survey.SurveyQuestions sq
JOIN Survey.Questions q ON sq.questionID = sq.questionID
JOIN Survey.SurveyResults sr ON sq.surveyQuestionsID = sr.surveyQuestionsID</CommandText>
